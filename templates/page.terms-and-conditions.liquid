        <div class="col-md-12 lazyload-element">
                 <div class="w-btn-wrapper">
                    <a class="w-btn icon_atleft" href="{{ shop.url }}"><span class="w-btn-label font-bold"><em>‚Üê</em> Back to Home</span></a>
                 </div>
               </div>
              {% comment %}
