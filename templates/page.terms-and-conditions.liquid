          <li class="home">
            <a href="/" title="Go to Home Page">Home</a>
            <span>| </span>
          </li>
          <li class="cms_page">
            <strong class="font-bold">{{ page.title }}</strong>
          </li>
        </ul>
        {% endcomment %}
      </div>
    </div>
  </div>
   <div class="container dt-prl-30">
