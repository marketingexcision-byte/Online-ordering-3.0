            <div class="col-sm-6">
               <div class="contact-form-wrapper">
                  <h2>Send Us Your Enquiry</h2>
                  <div>

                     {% form 'contact' %}
                     {% if form.posted_successfully? %}
                     <p class="note form-success">
                        {{ 'contact.form.post_success' | t }}
                     </p>
                     {% endif %}
                     {% include 'form-errors-custom' %}
                     <div id="contactFormWrapper">
                        <div class="row">
                           <div class="col-sm-6">
                              <label for="contactFormFirstName">First Name*</label>
                              <input required type="text" id="contactFormFirstName" name="contact[firstname]" />
                           </div>
                           <div class="col-sm-6">
                              <label for="contactFormLastName">Last Name</label>
                              <input type="text" id="contactFormLastName" name="contact[lastname]" />
                           </div>
                        </div>
                        <div class="row">
                           <div class="col-sm-6">
                              <label for="contactFormPhone">Mobile Phone Number</label>
                              <input type="tel" id="contactFormPhone" name="contact[phone]" />
                           </div>
                           <div class="col-sm-6">
                              <label for="contactFormEmail">Email</label>
                              <input required type="email" id="contactFormEmail" name="contact[email]" autocorrect="off" autocapitalize="off"autocorrect="off" autocapitalize="off" />
                           </div>
                        </div>
                       
                        <!--div class="row">
                           <div class="col-sm-6">
                              <label for="contactFormCity">City</label>
                              <input type="text" id="contactFormCity" name="contact[city]" />
                           </div>
                           <div class="col-sm-6">
                              <label for="contactFormCompanyName">Company Name</label>
                              <input type="text" id="contactFormCompanyName" name="contact[cname]" />
                           </div>
                        </div-->
                        
                       
                        <div class="row">
                           <div class="col-sm-12">
                              <label for="contactFormMessage">Message</label>
                              <textarea required id="contactFormMessage" rows="5" name="contact[message]"></textarea>
                           </div>
                        </div>
                        <div class="row">
                           <input type="submit" class="btn right" value="{{ 'contact.form.send' | t }}">
                        </div>
                     </div>

