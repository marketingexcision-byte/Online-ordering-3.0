<div class="container dt-prl-30">
   <div class="row">     
     {% if section.settings.enable_tab_1 %}
      <div class="col-12 col-sm-4 lazyload-element">
         <div class="card-2 card hoverable"> 
            <div class="card-image">
               <a href="{{ section.settings.tab_url_1 }}">
                 {% if section.settings.tab_image_1 != blank %}
                 <img src="{{ section.settings.tab_image_1 | img_url:'grande' }}" alt="">
                 {% endif %}
               </a>
            </div>
            <div class="card-content" style="height: auto;">
               <a href="{{ section.settings.tab_url_1 }}">
                 <h2>{{ section.settings.tab_title_1 }}</h2>
               </a>
            </div> 
            <a class="card-action" href="{{ section.settings.tab_url_1 }}">
               <p class="action font-bold">
