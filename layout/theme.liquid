<script>
  (function () {
    const preloader = document.getElementById('preloader');
    const loadedEl = preloader && preloader.querySelector('.loader-loaded');
    const unloadedEl = preloader && preloader.querySelector('.loader-unloaded');

    if (!preloader || !loadedEl || !unloadedEl) {
      if (preloader) {
        preloader.style.display = 'none';
      }
      return;
    }

    let progress = 0;
    let animationFrame;
    let completed = false;

    const reduceMotion = window.matchMedia('(prefers-reduced-motion: reduce)').matches;

    function updateBar(value) {
      const clamped = Math.min(value, 100);
      loadedEl.style.width = `${clamped}%`;
      unloadedEl.style.width = `${100 - clamped}%`;
    }

    function animateProgress() {
      if (progress < 90) {
        progress += Math.random() * 1.5; // simulate progressive load
        updateBar(progress);
        animationFrame = requestAnimationFrame(animateProgress);
      }
    }

    if (reduceMotion) {
      updateBar(100);
      preloader.style.opacity = '0';
      preloader.style.display = 'none';
    } else {
      // Start simulating progress
      requestAnimationFrame(animateProgress);
    }

    // Use PerformanceObserver to detect First Contentful Paint
    const po = new PerformanceObserver((entryList, observer) => {
      for (const entry of entryList.getEntries()) {
        if (entry.name === 'first-contentful-paint') {
          // Finish progress bar
          cancelAnimationFrame(animationFrame);
          updateBar(100);
          const finish = () => {
            preloader.style.opacity = '0';
            setTimeout(() => (preloader.style.display = 'none'), 300);
          };
          if (reduceMotion) {
            finish();
          } else {
            setTimeout(finish, 200);
          }
          observer.disconnect();
        }
      }
    });

    try {
      po.observe({ type: 'paint', buffered: true });
    } catch (e) {
      // Fallback if unsupported
      window.addEventListener('DOMContentLoaded', () => {
        updateBar(100);
        const finish = () => {
          preloader.style.opacity = '0';
          setTimeout(() => (preloader.style.display = 'none'), 300);
        };
        if (reduceMotion) {
          finish();
        } else {
          setTimeout(finish, 200);
        }
      });
    }
  })();
</script>

