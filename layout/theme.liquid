  <head>
    {% liquid
      assign robots_directive = ''
      assign googlebot_directive = ''
      assign request_path = request.path | default: '' | downcase
      assign request_query = request.query_string | default: '' | downcase
      assign has_collection_filters = false

      if request.page_type == 'collection' or template contains 'collection'
        if current_tags != blank
          assign has_collection_filters = true
        endif

        if has_collection_filters == false
          if request_query contains 'filter.' or request_query contains 'filter%5b'
            assign has_collection_filters = true
          elsif request_query contains 'pf_'
            assign has_collection_filters = true
          elsif request_query contains 'constraint='
            assign has_collection_filters = true
          elsif request_query contains 'options%5b'
            assign has_collection_filters = true
          else
            assign collection_path_parts = request_path | split: '/collections/'
            if collection_path_parts.size > 1
              assign collection_path_tail = collection_path_parts[1]
              assign collection_segments = collection_path_tail | split: '/'
              if collection_segments.size > 1
                assign collection_filter_segment = collection_segments[1]
                if collection_filter_segment contains '+'
                  assign has_collection_filters = true
                endif
              endif
            endif
          endif
        endif
      endif

      if has_collection_filters
        assign robots_directive = 'noindex, follow'
      endif

      if request_path contains 'compare'
        assign robots_directive = 'noindex, follow'
      endif

      if request_path contains 'search'
        assign robots_directive = 'noindex, follow'
      endif

      if robots_directive != '' and googlebot_directive == ''
        assign googlebot_directive = robots_directive
      endif
    %}

